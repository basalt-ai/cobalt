{
	"name": "@basalt-ai/cobalt",
	"version": "0.2.0",
	"description": "Unit testing for AI Agents â€” test, evaluate, and track your AI experiments",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"bin": {
		"cobalt": "./dist/cli.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"files": ["dist", "templates", "README.md"],
	"keywords": [
		"ai",
		"testing",
		"evaluation",
		"llm",
		"agents",
		"benchmark",
		"llm-judge",
		"ai-testing",
		"agent-testing"
	],
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "pnpm build:lib && pnpm build:dashboard",
		"build:lib": "tsup",
		"build:dashboard": "vite build -c src/dashboard/ui/vite.config.ts",
		"dev:dashboard": "vite -c src/dashboard/ui/vite.config.ts",
		"dev": "tsup --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "biome check .",
		"format": "biome format --write .",
		"check": "biome check --write ."
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.74.0",
		"@hono/node-server": "^1.19.9",
		"@modelcontextprotocol/sdk": "^1.26.0",
		"@phosphor-icons/react": "^2.1.10",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-popover": "^1.1.15",
		"@radix-ui/react-scroll-area": "^1.2.10",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-switch": "^1.2.6",
		"@radix-ui/react-tabs": "^1.1.13",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@tanstack/react-table": "^8.21.3",
		"autoevals": "^0.0.131",
		"better-sqlite3": "^12.6.2",
		"citty": "^0.1.6",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"defu": "^6.1.4",
		"dotenv": "^16.6.1",
		"hono": "^4.11.7",
		"jiti": "^2.4.2",
		"openai": "^5.0.0",
		"ora": "^8.1.1",
		"p-limit": "^6.1.0",
		"p-map": "^7.0.3",
		"pathe": "^1.1.2",
		"picocolors": "^1.1.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router": "^7.0.0",
		"recharts": "^3.7.0",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@cobalt/tsconfig": "workspace:*",
		"@tailwindcss/vite": "^4.1.18",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^22.10.5",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@vitejs/plugin-react": "^4.3.0",
		"@vitest/coverage-v8": "^2.1.8",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"vite": "^6.0.0",
		"vitest": "^2.1.8"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/basalt-ai/cobalt.git"
	},
	"license": "MIT"
}
