{
	"name": "cobalt",
	"version": "0.1.0",
	"private": true,
	"description": "Cobalt - Jest for AI Agents. Test, evaluate, and track your AI experiments.",
	"scripts": {
		"dev": "turbo dev",
		"build": "turbo build",
		"test": "turbo test",
		"test:watch": "turbo test:watch",
		"test:coverage": "turbo test:coverage",
		"lint": "turbo lint",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"check": "biome check --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"turbo": "^2.3.3"
	},
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=8.0.0"
	},
	"packageManager": "pnpm@8.15.4"
}
